<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>

		<PackageId>MoneyExtensionPedro</PackageId>
		<License>MIT</License>
		<Version>1.0.1</Version>
		<Authors>Pedro Souza</Authors>
		<PackageDescription>Extension to convert decimal to integer</PackageDescription>
		<RepositoryUrl>https://github.com/Phses/money-extension</RepositoryUrl>
		<PackageTags>money;int;decimal;converter</PackageTags>
		<Title>MoneyExtensions</Title>
		<PackageIcon>money.png</PackageIcon>
	</PropertyGroup>
	<ItemGroup>
		<None Include="..\Media\money.png" Pack="true" PackagePath="\"/>
	</ItemGroup>

</Project>
